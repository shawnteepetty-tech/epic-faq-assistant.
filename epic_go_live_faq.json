[
  {
    "module": "CardioServer – Tech User",
    "category": "Cardiology",
    "roles": ["Tech User"],
    "steps": [
      {
        "title": "Access CardioServer",
        "items": [
          "In Epic, open the patient chart → click the CardioServer SmartLink/URL.",
          "OR on the desktop, double-click the CardioServer icon.",
          "OR in Citrix Workspace → CardioServer → Open.",
          "On the login page, enter AD username + password.",
          "Confirm header shows Role = Tech User."
        ]
      },
      {
        "title": "Find & Filter Studies",
        "items": [
          "Open Worklist / Studies view.",
          "Set Facility/Location filter.",
          "Set Status filter: Preliminary / Unconfirmed / Confirmed / Stored.",
          "Optional: set Date filter (Today, Last 7 Days, Custom).",
          "Click Apply / Search."
        ]
      },
      {
        "title": "Link Study to Correct Patient & Order",
        "items": [
          "In Worklist, click the study.",
          "Click Link to Order / Reconcile.",
          "Search MRN or Name → select patient.",
          "If multiple orders show, select the correct order.",
          "Click Save / Link → verify patient + order appear in header."
        ]
      },
      {
        "title": "Assign Study to Provider",
        "items": [
          "Open study details.",
          "Click Provider / Assign field.",
          "Type provider name → select from list.",
          "Click Save / Update → confirm provider shows in header."
        ]
      },
      {
        "title": "Trash & Restore Studies",
        "items": [
          "Open study → confirm Status = Preliminary.",
          "Click More Actions → Move to Trash.",
          "To restore (≤30 days): open Trash → select study → Restore / Untrash."
        ]
      },
      {
        "title": "Manual Upload – ECG",
        "items": [
          "Click Upload / Import → ECG Upload.",
          "Browse → select ECG file.",
          "Enter MRN → Upload / Import.",
          "Link to order + Assign provider."
        ]
      },
      {
        "title": "Manual Upload – Stress Test",
        "items": [
          "Click Upload / Import → Stress Upload.",
          "Browse → select stress file.",
          "Enter MRN → Upload / Import.",
          "Assign provider."
        ]
      },
      {
        "title": "Multi-Facility Workflows",
        "items": [
          "Open Facility filter in Worklist.",
          "Select one or more facilities.",
          "Click Apply."
        ]
      },
      {
        "title": "Study Status Guide",
        "items": [
          "Preliminary = new/editable.",
          "Unconfirmed = awaiting provider review/signature.",
          "Confirmed = provider signed.",
          "Stored = archived.",
          "Trash = deleted prelims, held 30 days."
        ]
      }
    ],
    "qa": [
      {
        "q": "What if a study doesn’t match any patient/order?",
        "a": "Use Link/Reconcile → search MRN → select correct order."
      },
      {
        "q": "Can I trash a study that’s already Unconfirmed?",
        "a": "No, only Preliminary studies can be trashed."
      },
      {
        "q": "How do I restore a deleted study?",
        "a": "Go to Trash view → select study → Restore (within 30 days)."
      }
    ]
  },
  {
    "module": "EVC (Telehealth / Video Consults)",
    "category": "Telehealth",
    "roles": ["Provider", "Bedside Staff"],
    "steps": [
      {
        "title": "Launch a Consult",
        "items": [
          "In Epic, open the MPage 'TV Kit Consults' or go to the Converge website.",
          "Select patient/device.",
          "Click Start Consult."
        ]
      },
      {
        "title": "Accept Call (Bedside)",
        "items": [
          "On TV Kit 200S, click Accept.",
          "Verify audio and video."
        ]
      },
      {
        "title": "Provider Controls",
        "items": [
          "Start/End video as needed.",
          "Click Share Screen if presenting.",
          "Use Chat for text messages."
        ]
      },
      {
        "title": "Documentation",
        "items": [
          "In Epic, open the Consult MPage.",
          "Enter consult note → link to encounter → Sign."
        ]
      }
    ],
    "qa": [
      {
        "q": "What if patient can’t hear me?",
        "a": "Check mute, device volume, and TV Kit connection."
      },
      {
        "q": "Where do I document the visit?",
        "a": "In Epic under the Consult MPage note linked to the encounter."
      }
    ]
  },
  {
    "module": "ASAP (ED Track Board)",
    "category": "Emergency Department",
    "roles": ["Nurse", "Provider"],
    "steps": [
      {
        "title": "Track Board Overview",
        "items": [
          "Open ED Track Board.",
          "Review columns for Bed, Name, Age, Complaint, Acuity, Staff initials.",
          "Hover over initials for full staff names."
        ]
      },
      {
        "title": "Order Status Icons",
        "items": [
          "Check columns showing EKG, Meds, Labs, Imaging.",
          "Counts show Placed, In Process, Completed."
        ]
      },
      {
        "title": "Registration & Disposition",
        "items": [
          "Registration column: ✔ = complete, X = incomplete (does not block discharge).",
          "Disposition column = patient destination."
        ]
      },
      {
        "title": "Layout Customization",
        "items": [
          "Click-drag column headers to rearrange.",
          "Move items (e.g., Suicide Risk) to preferred position.",
          "Save layout if available."
        ]
      },
      {
        "title": "Views",
        "items": [
          "Toggle Side-by-Side vs Summary view.",
          "Click magnifying glass for more info."
        ]
      }
    ],
    "qa": [
      {
        "q": "What does an X in registration mean?",
        "a": "Registration incomplete. It won’t block discharge."
      },
      {
        "q": "How do I see pending labs?",
        "a": "Check order columns: Placed, In Process, Completed."
      }
    ]
  }
,
  {
    "module": "Beaker (Lab Workflow)",
    "category": "Laboratory",
    "roles": ["Phlebotomist", "Lab Tech"],
    "steps": [
      {
        "title": "Specimen Collection",
        "items": [
          "Check-in patient → enter orders.",
          "Print specimen labels.",
          "Scan wristband → scan specimen.",
          "Transport to lab."
        ]
      },
      {
        "title": "Receiving & Processing",
        "items": [
          "Scan barcode in Receiving.",
          "Verify orders/containers.",
          "Add FYIs or cancel/redraw if needed."
        ]
      },
      {
        "title": "Tech Workflow",
        "items": [
          "Use Outstanding List.",
          "Repeat tests if needed.",
          "Verify results.",
          "Document critical values."
        ]
      },
      {
        "title": "Send-Outs",
        "items": [
          "Create packing list.",
          "Add specimens.",
          "Assign destination.",
          "Mark ready/picked up."
        ]
      },
      {
        "title": "Reports",
        "items": [
          "Use Specimen Inquiry for status.",
          "Print reports or labels."
        ]
      }
    ],
    "qa": [
      {
        "q": "How do I reprint a specimen label?",
        "a": "Open the Order → select Reprint Label."
      },
      {
        "q": "Where do I check pending tests?",
        "a": "Check the Outstanding List."
      }
    ]
  },
  {
    "module": "Optime Anesthesia",
    "category": "Surgery/Anesthesia",
    "roles": ["Anesthesiologist", "CRNA", "Resident"],
    "steps": [
      {
        "title": "Login",
        "items": [
          "Enter Epic credentials.",
          "Confirm Department and Role."
        ]
      },
      {
        "title": "Electronic Consent",
        "items": [
          "Open Consent activity.",
          "Select consent type.",
          "Capture e-signatures.",
          "If refusal: print, ink sign, scan, send to HIM."
        ]
      },
      {
        "title": "Specimen Labeling",
        "items": [
          "Place order.",
          "Print labels.",
          "Scan wristband + specimen."
        ]
      },
      {
        "title": "Intra-Op Workflow",
        "items": [
          "Use Sidebar Quick Events: In Room, Procedure Start, etc.",
          "Document events as they occur."
        ]
      },
      {
        "title": "PACU Workflow",
        "items": [
          "Enter dressing and laterality.",
          "Add PACU comments."
        ]
      }
    ],
    "qa": [
      {
        "q": "How do I substitute a signer on consent?",
        "a": "Enter alternate signer name and add a nurse witness."
      },
      {
        "q": "What if patient refuses e-signature?",
        "a": "Print the form, capture ink signature, scan, send to HIM."
      }
    ]
  },
  {
    "module": "Inpatient Provider Workflow",
    "category": "Inpatient",
    "roles": ["Hospitalist", "Resident", "Consultant"],
    "steps": [
      {
        "title": "Navigation",
        "items": [
          "Open My Lists.",
          "Review Storyboard: demographics, team, labs."
        ]
      },
      {
        "title": "Problem List & Orders",
        "items": [
          "Update/add/resolve problems.",
          "Place or review orders."
        ]
      },
      {
        "title": "Medication Reconciliation",
        "items": [
          "Perform at Admission.",
          "Repeat at Transfer.",
          "Finalize at Discharge."
        ]
      },
      {
        "title": "Discharge Workflow",
        "items": [
          "Review allergies/problems.",
          "Reconcile meds.",
          "Generate AVS."
        ]
      },
      {
        "title": "Consultant Workflow",
        "items": [
          "Create consult note.",
          "Use speed buttons/smart phrases.",
          "Sign note."
        ]
      }
    ],
    "qa": [
      {
        "q": "Where do I reconcile medications?",
        "a": "During admission, transfer, and discharge."
      },
      {
        "q": "What if there’s a hard stop in an order set?",
        "a": "You must address it before proceeding."
      }
    ]
  }
,
  {
    "module": "Pharmacy (Willow)",
    "category": "Pharmacy",
    "roles": ["Pharmacist", "Pharmacy Tech"],
    "steps": [
      {
        "title": "Dashboard & In Basket",
        "items": [
          "Open Pharmacy Dashboard.",
          "Check In Basket: refills, reminders, time changes."
        ]
      },
      {
        "title": "Dispense Functions",
        "items": [
          "Click Dispense.",
          "Choose Prep, Cart Fill, Compounding, Repackaging, or Barcode Validation."
        ]
      },
      {
        "title": "Medication List & ADS Tab",
        "items": [
          "Open Medication List.",
          "Check ADS tab for IID.",
          "If blank, submit ticket."
        ]
      },
      {
        "title": "Cart Fill Troubleshooting",
        "items": [
          "Reprint missing labels via Dispense.",
          "If printer is down, reroute jobs.",
          "Remove rerouted printers when resolved."
        ]
      },
      {
        "title": "Barcode Validation",
        "items": [
          "Scan Cardinal deliveries.",
          "Verify NDC matches.",
          "Submit mismatches with details (name, strength, form, mfr, NDC, cost)."
        ]
      }
    ],
    "qa": [
      {
        "q": "What if cart fill labels don’t print?",
        "a": "Manually reprint via the Dispense button."
      },
      {
        "q": "What if IID is blank?",
        "a": "Submit a ticket for IID assignment."
      }
    ]
  },
  {
    "module": "Ambulatory Clinic",
    "category": "Outpatient/Ambulatory",
    "roles": ["Physician", "Medical Assistant", "Clinic Staff"],
    "steps": [
      {
        "title": "Learning Home",
        "items": [
          "Press F1 in Epic.",
          "Access Learning Home Dashboard.",
          "Locate tip sheets/resources."
        ]
      },
      {
        "title": "Schedule & Status",
        "items": [
          "Open full clinic schedule.",
          "Track progress in Status column."
        ]
      },
      {
        "title": "Rooming Workflow",
        "items": [
          "Set preferred lab during rooming.",
          "Complete vitals and screenings."
        ]
      },
      {
        "title": "Charge Capture",
        "items": [
          "Automatic: meds, immunizations, POCT.",
          "Manual: procedures, supplies.",
          "Confirm billing provider."
        ]
      },
      {
        "title": "Referrals",
        "items": [
          "Internal vs external referrals.",
          "If provider missing, enroll using FIND tool."
        ]
      }
    ],
    "qa": [
      {
        "q": "How do I activate MyChart?",
        "a": "Send activation via text/email; valid 14 days."
      },
      {
        "q": "How do I schedule telemedicine?",
        "a": "Use Telemedicine visit type."
      }
    ]
  },
  {
    "module": "Go-Live Preparation",
    "category": "Implementation",
    "roles": ["All Staff"],
    "steps": [
      {
        "title": "Go-Live Overview",
        "items": [
          "Go-live date: October 1.",
          "Saint Francis Evanston + Holy Family Medical Center.",
          "Holy Family is first LTACH (long-term care) in Prime."
        ]
      },
      {
        "title": "New Features",
        "items": [
          "Meds to Bed program.",
          "Epic Narrators for code, rapid response, sepsis, trauma, etc.",
          "E-signature replaces paper consents.",
          "Rovers not used for meds/wounds; use cameras + scanners."
        ]
      },
      {
        "title": "Clinical Documentation",
        "items": [
          "SDOH with BPA-triggered consults.",
          "Specimen collection with specimen labels.",
          "Patient expiration navigator with donor forms.",
          "Belongings tracked with e-signature.",
          "Critical values documented with provider read-back."
        ]
      },
      {
        "title": "Specialty Workflows",
        "items": [
          "Wound care with Four Eyes Assessment.",
          "Device association in critical care.",
          "Therapies structured in navigators/flowsheets.",
          "Dietitian/malnutrition workflows with auto-calcs.",
          "Charge capture for dialysis and wound care."
        ]
      },
      {
        "title": "Case Management & Social Services",
        "items": [
          "Resolve admission vs observation conflicts.",
          "Follow up on SDOH BPAs.",
          "Track discharge milestones.",
          "Use CCSC navigator for referrals.",
          "Complete EMTALA for inter-facility transfers."
        ]
      },
      {
        "title": "Holy Family LTACH Workflows",
        "items": [
          "Complete LTCH care data set at admit/discharge.",
          "Transfers from Prime require discharge + readmit.",
          "Leave of Absence workflows for infusions/visits.",
          "Emergency transfers handled as discharges.",
          "MRI workflow includes checklist + e-signature.",
          "Infection control flags now result-triggered."
        ]
      },
      {
        "title": "Outstanding Items",
        "items": [
          "Updated tip sheets to be distributed.",
          "Finalize intake coordinator workflows.",
          "Help desk tickets for missing referral facilities.",
          "Include LTACH and MRI tip sheets in repository."
        ]
      }
    ],
    "qa": [
      {
        "q": "What’s unique about Holy Family?",
        "a": "It’s the first Prime LTACH with distinct workflows."
      },
      {
        "q": "Where can I find tip sheets?",
        "a": "They will be emailed and stored in the shared repository."
      }
    ]
  }
